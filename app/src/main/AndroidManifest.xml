<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hci.com.vocaagent" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".MainFragmentActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name=".ListNotesActivity"
        android:parentActivityName=".MainFragmentActivity">

    </activity>
    <activity android:name=".BookActivity"
        android:parentActivityName=".ListNotesActivity">

        </activity>
        <activity android:name=".SelectBookActivity"
            android:parentActivityName=".MainFragmentActivity">

        </activity>
        <activity android:name=".ExamPagerActivity">

        </activity>
    </application>

</manifest>
